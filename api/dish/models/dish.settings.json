{
  "kind": "collectionType",
  "collectionName": "dishes",
  "info": {
    "name": "Dish"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "uid": {
      "type": "uuid"
    },
    "name": {
      "type": "string"
    },
