      "required": false
    },
    "price": {
      "type": "integer"
    },
    "stripeId": {
      "type": "string"
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "regular",
        "large"
      ]
    },
    "restaurant": {
      "model": "restaurant",
      "via": "dishes"
    },
    "category": {
      "model": "category",
      "via": "dishes"
    },
    "apiId": {
      "type": "string"
    }
  }
